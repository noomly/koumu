{
    "name": "koumu",
    "version": "0.2.0",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "author": "Eyal Chojnowski <eyal@pianity.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/noomly/koumu.git"
    },
    "bin": "build/cli.js",
    "files": [
        "build",
        "README.md"
    ],
    "scripts": {
        "typecheck": "tsc",
        "dev": "tsx src/cli/index.ts",
        "build": "tsx scripts/build.ts",
        "lint": "prettier --check src/ ; eslint src/",
        "lint:fix": "prettier --write src/ ; eslint --fix src/",
        "postinstall": "husky install && koumu setup --husky",
        "prepack": "pinst --disable",
        "postpack": "pinst --enable"
    },
    "dependencies": {
        "@iarna/toml": "3.0.0",
        "chalk": "5.0.1",
        "commander": "9.4.1",
        "esbuild": "0.15.10",
        "inquirer": "9.1.2",
        "inquirer-autocomplete-prompt": "3.0.0",
        "node-emoji": "1.11.0"
    },
    "devDependencies": {
        "@pianity/eslint-config": "0.0.2",
        "@pianity/prettier-config": "0.0.1",
        "@types/inquirer": "9.0.2",
        "@types/inquirer-autocomplete-prompt": "1.3.5",
        "@types/node": "18.7.23",
        "@types/node-emoji": "1.8.2",
        "eslint": "8.24.0",
        "husky": "8.0.1",
        "nodemon": "2.0.20",
        "pinst": "3.0.0",
        "postinstall-postinstall": "2.1.0",
        "prettier": "2.7.1",
        "tsconfig-paths": "4.1.0",
        "tsx": "3.9.0",
        "typescript": "4.8.4"
    },
    "packageManager": "yarn@3.2.0"
}
